                "name": "Afferent role"
            },
            {
                "id": "NIFMOL:sao185580330",
                "name": "Acetylcholine"
            },
            {
                "id": "NIFORG:birnlex_266",
                "name": "Sprague Dawley"
            }
        ],
        "version": "1"
